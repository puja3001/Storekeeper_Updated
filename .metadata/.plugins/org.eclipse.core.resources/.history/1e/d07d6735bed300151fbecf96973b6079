package sg.edu.nus.iss.club;

import java.util.Date;

public class Booking {
	
	private Member member;
	private Facility facility;
	Date startDate;
	Date endDate;
	
	public Booking(Member m,Facility f,Date sd,Date ed){
		this.member = m;
		this.facility = f;
		this.startDate = sd;
		this.endDate = ed;
	}
	
	public boolean overlaps(Booking book){
		if(this.facility == book.facility){
			if(book.startDate.after(this.startDate) && book.startDate.before(this.endDate)){
				return true;
			}
		}
		
	}
	
}
