package sg.edu.iss.univsovsys;

public class Member {
	
	private String memId;
	private String memName;
	private int loyaltyPoints;
	private double totalSpent;
	private double remSpent;
	
	public Member(String memId,String memName,int loyaltyPoints,double totalSpent,double remSpent){
		this.memId = memId;
		this.memName = memName;
		this.loyaltyPoints = -1;
		this.totalSpent = 0;
		this.remSpent = 0;
	}

	public String getMemId(){
		return memId;
	}
	public String getMemName(){
		return memName;
	}
	public int getLoyaltyPoints(){
		return loyaltyPoints;
	}
	public double getTotalSpent(){
		return totalSpent;
		}
	public double getRemSpent(){
		return remSpent;
		}
}
